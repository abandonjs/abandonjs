{
  "name": "abandonjs",
  "version": "3.1.1",
  "description": "方法包",
  "module": "lib/index.esm.js",
  "main": "lib/index.js",
  "unpkg": "/lib/",
  "deliver": "/lib/",
  "typings": "lib/index.d.ts",
  "scripts": {
    "commitlint": "commitlint -e",
    "reload": "rimraf node_modules && pnpm install",
    "build": "npx rollup -c ./config/rollup.build.ts",
    "git": "node config/git",
    "build:ed": "npx rollup -c --extend ./rollup.config.ts",
    "docs": "node ./config/docs.js",
    "test": "ts-node-dev --respawn --transpile-only ./test/index.ts",
    "eslint": "eslint . --ext .ts",
    "pub": "npm publish"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "typescript",
    "javascript",
    "method"
  ],
  "engines": {
    "node": ">=16"
  },
  "bugs": "https://github.com/abandonjs/abandonjs/issues",
  "repository": {
    "type": "git",
    "url": "git@github.com:abandonjs/abandonjs.git"
  },
  "author": "ruihuag",
  "license": "MIT",
  "devDependencies": {
    "0math": "0.1.2",
    "@babel/core": "^7.21.8",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^23.0.7",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "asura-eye": "^0.6.1",
    "command-go": "^0.1.1",
    "enquirer": "^2.3.6",
    "eslint": "^8.31.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "node-notifier": "^10.0.1",
    "prettier": "^2.8.3",
    "rollup": "^2.79.1",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^5.1.6",
    "unit-testing-js": "1.0.0"
  }
}
