{
  "name": "rh-js-methods",
  "version": "2.1.12",
  "description": "方法包",
  "module": "lib/index.esm.js",
  "main": "lib/index.js",
  "unpkg": "/lib/",
  "jsdelivr": "/lib/",
  "typings": "lib/index.d.ts",
  "scripts": {
    "commitlint": "commitlint -e",
    "reload": "rimraf node_modules && cnpm install",
    "build": "npx rollup -c --extend ./rollup.config.ts",
    "lint-staged": "lint-staged",
    "md": "ts-node ./md.ts",
    "md:dev": "ts-node-dev --respawn --transpile-only ./md.ts",
    "test": "ts-node-dev --respawn --transpile-only ./test/index.ts",
    "restart": "rimraf dist && npm run build && npm start",
    "start": "node ./dist/index.js",
    "prod": "npm run build && npm run start",
    "eslint": "eslint . --ext .ts",
    "eslint:fix": "eslint . --ext .ts --fix",
    "patch": "npm version patch -f",
    "pub": "npm publish"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue,jsx,tsx}": [
      "prettier --write",
      "eslint --cache --fix",
      "git add"
    ]
  },
  "keywords": [
    "typescript",
    "javascript",
    "method"
  ],
  "engines": {
    "node": ">=16"
  },
  "author": "ruihuag",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@types/bluebird": "^3.5.36",
    "@types/node": "^18.7.13",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^12.3.2",
    "node-notifier": "^8.0.2",
    "prettier": "^2.4.1",
    "rh-test": "^0.1.17",
    "rimraf": "^3.0.2",
    "rollup": "^2.66.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^4.1.6"
  },
  "dependencies": {
    "rh-ts-methods": "^0.0.2"
  }
}