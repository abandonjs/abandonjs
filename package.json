{
  "name": "rh-js-methods",
  "version": "2.1.12",
  "description": "方法包",
  "module": "lib/index.esm.js",
  "main": "lib/index.js",
  "unpkg": "/lib/",
  "deliver": "/lib/",
  "typings": "lib/index.d.ts",
  "scripts": {
    "commitlint": "commitlint -e",
    "reload": "rimraf node_modules && cnpm install",
    "build": "npx rollup -c --extend ./rollup.config.ts",
    "md": "ts-node ./md.ts",
    "md:dev": "ts-node-dev --respawn --transpile-only ./md.ts",
    "test": "ts-node-dev --respawn --transpile-only ./test/index.ts",
    "restart": "rimraf dist && npm run build && npm start",
    "start": "node ./dist/index.js",
    "prod": "npm run build && npm run start",
    "eslint": "eslint . --ext .ts",
    "eslint:fix": "eslint . --ext .ts --fix",
    "pub": "npm publish",
    "nocache": "jest --no-cache",
    "watch": "jest --watchAll",
    "coverage": "jest --coverage",
    "jest": "jest --coverage",
    "verbose": "npx jest --verbose"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "typescript",
    "javascript",
    "method"
  ],
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:guanruihua/rh-js-methods.git"
  },
  "author": "ruihuag",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@types/bluebird": "^3.5.37",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.2",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "babel-jest": "^29.2.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "eslint": "^8.25.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.2.1",
    "node-notifier": "^8.0.2",
    "prettier": "^2.7.1",
    "rh-test": "0.1.24",
    "rh-ts-methods": "^0.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^1.1.8",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^4.8.4"
  }
}